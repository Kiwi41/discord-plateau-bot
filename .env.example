# Configuration Discord Bot Python
# Copier ce fichier vers .env et remplir avec vos vraies valeurs

# Token du bot Discord (depuis Discord Developer Portal)
DISCORD_TOKEN=your_discord_bot_token_here

# ID du serveur Discord (Guild ID)
GUILD_ID=your_guild_id_here

# ID du canal forum pour les posts
FORUM_CHANNEL_ID=your_forum_channel_id_here

# URL d'inscription pour les événements
REGISTRATION_URL=https://example.com/inscription

# ID de l'événement récurrent Discord (optionnel)
EVENT_ID=

# Fuseau horaire (défaut: Europe/Paris)
TIMEZONE=Europe/Paris
